                function mem_bar_chk() {
                    var u_id = document.getElementById("login_01_u_id");
                    var u_password = document.getElementById("login_01_u_password");
                    if (u_id.value == "") {
                        alert("請輸入帳號");
                        u_id.focus();
                        return false;
                    } else if (u_password.value == "") {
                        alert("請輸入密碼");
                        u_password.focus();
                        return false;
                    }
                    return true;
                 }
